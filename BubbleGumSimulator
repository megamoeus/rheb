    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("REC HUB- Bubble Gum Simulator", "BloodTheme")

    
-- Player
    local Main = Window:NewTab("Dupe Options")
    local MainSection = Main:NewSection("Dupes")


        MainSection:NewToggle("Dupe 1-- Turn on for 12 seconds then turn off in trade menu", "Turn on for 12 seconds then turn off in trade menu", function(v)
        getgenv().buybelts = v
        while wait() do
        if not getgenv().buybelts then return end
		function Death()
			spawn(function()
				local args = {
					[1] = "GetPlayerData"
				}

				game:GetService("ReplicatedStorage").NetworkRemoteFunction:InvokeServer(unpack(args))

			end)
		end

		for i=1,25000 do
			spawn(function()
				Death() 

			end)
		end




                end
        end)
