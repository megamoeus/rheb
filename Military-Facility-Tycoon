    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("REC Hub", "Sentinel")

    
    -- MAIN
    local Main = Window:NewTab("Money")
    local MainSection = Main:NewSection("Money Options")
    
            MainSection:NewButton("Give Money", "Gives Money", function()
        game.ReplicatedStorage.EnterCode:FireServer(7521242141241242465215165215)
    end)
    
        MainSection:NewToggle("Money Loop", "Loop Gives Money", function(v)
        getgenv().buybelts = v
        while wait() do
            if not getgenv().buybelts then return end
            game.ReplicatedStorage.EnterCode:FireServer(7521242141241242465215)
                end
        end)
    
    
-- Player
    local Player = Window:NewTab("Player")
    local PlayerSection = Player:NewSection("Player Options")

    PlayerSection:NewSlider("Walkspeed", "Changes the walkspeed", 250, 16, function(v)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
    end)

    PlayerSection:NewSlider("Jumppower", "Changes the jumppower", 250, 50, function(v)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
    end)

    PlayerSection:NewButton("Kill", "Kills Yourself", function()
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
    end)
    
    
    
    -- Miscellaneous
    local Misc = Window:NewTab("Misc")
    local Miscellaneous = Misc:NewSection("Miscellaneous")


    Miscellaneous:NewButton("Show Health Development Purposes", "Shows Health in F9", function()
        print(game.Players.LocalPlayer.Character.Humanoid.Health)
    end)

